{
  "name": "kubernetes-external-secrets",
  "version": "5.1.0",
  "description": "Kubernetes external secrets",
  "main": "build/bin/daemon.js",
  "scripts": {
    "coverage": "nyc ./node_modules/mocha/bin/_mocha --recursive src",
    "local": "LOCALSTACK=1 AWS_ACCESS_KEY_ID=foobar AWS_SECRET_ACCESS_KEY=foobar nodemon",
    "localstack": "docker run -it -p 4583:4583 -p 4584:4584 -p 4592:4592 -p 9999:8080 -e SERVICES=ssm,secretsmanager,sts -e DEBUG=1 --rm localstack/localstack:0.10.5",
    "release": "standard-version --tag-prefix='' && ./release.sh",
    "start": "node ./build/bin/daemon.js",
    "nodemon": "ts-node --files ./src/bin/daemon.ts",
    "test": "mocha -r ts-node/register \"src/**/*.test.ts\"",
    "test-e2e": "./e2e/run-e2e-suite.sh",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/kubernetes-external-secrets"
  },
  "keywords": [
    "kubernetes",
    "secrets",
    "aws",
    "alicloud",
    "aliyun"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@alicloud/kms20160120": "^1.1.0",
    "@azure/identity": "^1.0.3",
    "@azure/keyvault-secrets": "^4.0.4",
    "@google-cloud/secret-manager": "^1.2.1",
    "aws-sdk": "^2.628.0",
    "express": "^4.17.1",
    "js-yaml": "^3.13.1",
    "json-stream": "^1.0.0",
    "kubernetes-client": "^9.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "make-promises-safe": "^5.1.0",
    "node-vault": "^0.9.18",
    "pino": "^6.4.1",
    "prom-client": "^12.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.7",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/mocha": "^8.0.1",
    "@types/node": "^13.11.1",
    "@types/pino": "^6.3.0",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.10",
    "chai": "4.2.0",
    "dotenv": "8.2.0",
    "eslint": "^6.8.0",
    "gts": "^2.0.2",
    "mocha": "7.2.0",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "sinon": "9.0.2",
    "standard-version": "8.0.2",
    "supertest": "4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "nyc": {
    "check-coverage": true,
    "reporter": [
      "cobertura",
      "json-summary",
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "config/",
      "coverage/",
      "bin/",
      "**/*.test.js"
    ],
    "lines": 4,
    "functions": 4,
    "all": true,
    "cache": false,
    "temp-directory": "./coverage/.nyc_output"
  }
}
